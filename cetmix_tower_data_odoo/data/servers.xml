<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- SERVER: template_odoo -->
    <record id="server_template_odoo" model="cx.tower.server">
        <!-- <field name="module">cetmix_tower_data_odoo</field> -->
        <field name="name">template_odoo</field>
        <field name="ssh_username">admin</field>
        <field name="ssh_password">admin</field>
        <field name="ip_v4_address">1.2.3.4</field>
    </record>
    <record id="server_template_odoo_variable_instance_name" model="cx.tower.variable.value">
        <!-- <field name="module">cetmix_tower_data_odoo</field> -->
        <field name="server_id" ref="server_template_odoo" />
        <field name="variable_id" ref="variable_instance_name" />
        <field name="value_char"></field>
    </record>
    <record id="server_template_odoo_variable_odoo_version" model="cx.tower.variable.value">
        <!-- <field name="module">cetmix_tower_data_odoo</field> -->
        <field name="server_id" ref="server_template_odoo" />
        <field name="variable_id" ref="variable_odoo_version" />
        <field name="value_char"></field>
    </record>
    <record id="server_template_odoo_variable_odoo_port" model="cx.tower.variable.value">
        <!-- <field name="module">cetmix_tower_data_odoo</field> -->
        <field name="server_id" ref="server_template_odoo" />
        <field name="variable_id" ref="variable_odoo_port" />
        <field name="value_char"></field>
    </record>

</odoo>
