<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- SERVER: template-traefik -->
    <record id="server_template-traefik" model="cx.tower.server">
        <field name="name">template-traefik</field>
        <field name="ssh_username">admin</field>
        <field name="ssh_password">admin</field>
        <field name="ip_v4_address">1.2.3.4</field>
    </record>
    <record id="server_template-traefik_variable_traefik_image_name" model="cx.tower.variable.value">
        <field name="module">cetmix_tower_data_traefik</field>
        <field name="server_id" ref="server_template-traefik" />
        <field name="variable_id" ref="variable_traefik_image_name" />
        <field name="value_char">{{ traefik_instance_name }}</field>
    </record>
    <record id="server_template-traefik_variable_traefik_image_tag" model="cx.tower.variable.value">
        <field name="module">cetmix_tower_data_traefik</field>
        <field name="server_id" ref="server_template-traefik" />
        <field name="variable_id" ref="variable_traefik_image_tag" />
        <field name="value_char">v3.0</field>
    </record>
    <record id="server_template-traefik_variable_traefik_instance_name" model="cx.tower.variable.value">
        <field name="module">cetmix_tower_data_traefik</field>
        <field name="server_id" ref="server_template-traefik" />
        <field name="variable_id" ref="variable_traefik_instance_name" />
        <field name="value_char">shared-traefik</field>
    </record>
    <record id="server_template-traefik_variable_traefik_email" model="cx.tower.variable.value">
        <field name="module">cetmix_tower_data_traefik</field>
        <field name="server_id" ref="server_template-traefik" />
        <field name="variable_id" ref="variable_traefik_email" />
        <field name="value_char"></field>
    </record>
    <record id="server_template-traefik_variable_traefik_url" model="cx.tower.variable.value">
        <field name="module">cetmix_tower_data_traefik</field>
        <field name="server_id" ref="server_template-traefik" />
        <field name="variable_id" ref="variable_traefik_url" />
        <field name="value_char">{{ traefik_url_staging }}</field>
    </record>

</odoo>
